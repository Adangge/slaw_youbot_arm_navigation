<launch>
  <group ns="arm_1/arm_controller">
    <rosparam file="$(find slaw_arm_navigation)/config/joints_configuration.yaml" command="load" />

    <node pkg="slaw_arm_navigation" type="joint_trajectory_action.py" name="joint_trajectory_action" output="screen">
      <rosparam file="$(find slaw_arm_navigation)/config/joints_configuration.yaml" command="load" />
      <remap from="joint_states" to="/joint_states"/>
    </node>
    <node pkg='slaw_arm_navigation' type='joint_velocity_action.py' name='velocity_controller' output='screen' />
  </group>
  <node pkg='slaw_arm_navigation' type='side_joint_publisher.py' name='side_joint' output='screen' />

</launch>
